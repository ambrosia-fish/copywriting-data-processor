{
  "output": {
    "csv": {
      "path": "data/newsletters.csv"
    }
  },
  "substack": {
    "keywords": [
      "copywriting",
      "marketing",
      "email marketing",
      "content marketing"
    ],
    "complete_data_only": true
  },
  "processing": {
    "subscriber_count": {
      "rounding": {
        "less_than_1000": 100,
        "less_than_10000": 1000,
        "less_than_500000": 10000,
        "above_500000": 100000,
        "million_plus": "1 Million+"
      }
    },
    "email_verification": {
      "enabled": true,
      "method": "basic"
    },
    "limit": 3000
  }
}